<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.faint.persistence.LoginDao">

	<select id="find_by_string_id" parameterType="String" resultType="com.faint.dto.LoginDTO">
		SELECT * FROM tbl_user WHERE email = #{email}
